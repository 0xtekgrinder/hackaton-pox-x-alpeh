{
  "name": "nextjs-template",
  "version": "1.0.0",
  "description": "A template for NextJS projects.",
  "author": "Milkyway Technology",
  "repository": "https://github.com/milkyway-technology/nextjs-template",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "start:dev": "next dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "start-test start:dev http://localhost:3000 'cypress run --config-file ./test/cypress.config.ts'",
    "test:e2e:watch": "start-test start:dev http://localhost:3000 'cypress open --config-file ./test/cypress.config.ts'"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "autoprefixer": "^10.4.16",
    "next": "^14.0.4",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.4",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/cypress__code-coverage": "^3.10.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "cypress": "^13.6.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "start-server-and-test": "^2.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
